nextflow_pipeline {

    name "Test Workflow main.nf"
    script "main.nf"
    tag "test_mouse"
    tag "pipeline"

    test("test mouse alleles within download path (allele prediction reduction)") {

        when {
            params {
                outdir = "$outputDir"
                ncbi_email = "$NCBI_EMAIL"
                ncbi_key = "$NCBI_KEY"
            }
        }

        then {
            assert workflow.success
        }

    }

}
